{
  "$schema": "vscode://schemas/tasks",
  "version": "2.0.0",
  "tasks": [
    {
      "label": "run tests",
      "type": "shell",
      "command": "& \"${workspaceFolder}/.venv/Scripts/python.exe\" -m unittest discover -s tests -p \"test_*.py\"",
      "group": "test",
      "problemMatcher": []
    }
  ]
}
